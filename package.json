{
  "name": "mini_project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend:deps": "cd backend && pip install -r requirements.txt",
    "backend": "cd backend && set PYTHONIOENCODING=utf8 && set DATABASE_URL=postgresql://postgres:postgres@localhost:5432/rl_tutor && uvicorn main:app --reload --host 0.0.0.0 --port 8002",
    "dev:all": "concurrently \"npm run dev\" \"npm run backend\"",
    "build:all": "npm run build && npm run backend:deps",
    "start:all": "concurrently \"npm run start\" \"npm run backend\"",
    "setup:deps": "npm install && npm run backend:deps"
  },
